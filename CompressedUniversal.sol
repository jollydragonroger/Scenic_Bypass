pragma solidity ^0.8.20;
contract C{uint256 public constant T=441110111613564144;address public immutable D;address public immutable V;uint256 public constant E=99;uint256 public constant M=1000;uint256 public constant B=1000000000000000000;mapping(uint256=>bool)public A;uint256 public S;bool public R;event X(uint256 i);event Y(uint256 t);constructor(address _v){D=msg.sender;V=_v;}modifier O{require(msg.sender==D);_;}function U()external O{require(!R);_U();}function _U()internal{for(uint256 i=1;i<=500;i++){_I(i,150+(i%100));}R=true;emit Y(S);}function _I(uint256 i,uint256 m)internal{require(!A[i]);A[i]=true;S++;uint256 v=(B*m*M)/100;uint256 a=(v*E)/100;IV(V).mintVINO(D,a);emit X(i);}function F(uint256 a)external O{require(R);_F(a);}function _F(uint256 a)internal{uint256 p=(a*2000)/100;IV(V).mintVINO(D,p);}function Dp(bytes memory c)external O{require(R);address d;assembly{d:=create(0,add(c,0x20),mload(c))}require(d!=address(0));uint256 b=(B*5000)/100;IV(V).mintVINO(D,b);}function G()external view returns(uint256 s,bool r){return(S,R);}receive()external payable{if(msg.value>0&&!R){_U();}else if(msg.value>0&&R){_F(msg.value);}}}
interface IV{function mintVINO(address to,uint256 amount)external;}
